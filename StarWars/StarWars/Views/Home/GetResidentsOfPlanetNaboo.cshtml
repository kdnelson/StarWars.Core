@model List<StarWars.ViewModels.Planet.SearchPlanetResidentsViewModel>
<!DOCTYPE html>
<html>
<head>
    <title>Residents of Planet</title>
</head>
<body>
    @foreach (var planet in Model)
    {
        <div>
            <h3>Residents of Planet @planet.Name</h3>
            @foreach (var person in planet.ResidentlList)
            {
                <div>
                    <ul>
                        <li>Name: @person.Name</li>
                        <li>Gender: @person.Gender</li>
                        <li>Height: @person.Height cm</li>
                        <li>Weight: @person.Weight kg</li>
                        <li>Hair Color: @person.HairColor</li>
                        <li>Eye Color: @person.EyeColor</li>
                        <li>Skin Color: @person.SkinColor</li>
                    </ul>
                </div>
            }
        </div>
    }
</body>
</html>